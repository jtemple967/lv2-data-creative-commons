@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-range>
a lv2:Plugin, mod:ControlVoltagePlugin;

doap:name "CV Range Divider";
doap:license "GPL v2+";
doap:developer [
    foaf:name "Bram Giesen";
    foaf:homepage <>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

    lv2:minorVersion 3;
    lv2:microVersion 0;

rdfs:comment """
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 0;
    lv2:symbol "CvInput";
    lv2:name "CV Input";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 1;
    lv2:symbol "Cvoutput1";
    lv2:name "CV Output1";
],
[
    a lv2:OutputPort, lv2:CVPort;
    lv2:index 2;
    lv2:symbol "Cvoutput2";
    lv2:name "CV Output2";
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "MinRange" ;
    lv2:name "MinRange";
    lv2:default 0.0 ;
    lv2:minimum -100.0 ;
    lv2:maximum 100.0 ;
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "MaxRange" ;
    lv2:name "MaxRange";
    lv2:default 100.0 ;
    lv2:minimum -100.0 ;
    lv2:maximum 100.0 ;
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 5 ;
    lv2:symbol "ModeZero" ;
    lv2:name "ModeZero";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
    lv2:portProperty lv2:toggled;
]
.
