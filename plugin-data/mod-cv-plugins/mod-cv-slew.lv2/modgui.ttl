@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-slew>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-cv-slew.html> ;
        modgui:stylesheet <modgui/stylesheet-cv-slew.css> ;
        modgui:screenshot <modgui/mod_cv_slew_ss.png> ;
        modgui:thumbnail <modgui/mod_cv_slew_tn.png> ;
        modgui:brand "MOD" ;
        modgui:label "Slew Rate Limiter" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "RiseTime" ;
            lv2:name "Rise Time" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "FallTime" ;
            lv2:name "Fall Time" ;
        ] ;
    ] .
