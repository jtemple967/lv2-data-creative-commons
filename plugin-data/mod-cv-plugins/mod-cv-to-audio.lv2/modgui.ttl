@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mod-devel/mod-cv-to-audio>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-cv-to-audio.html> ;
        modgui:stylesheet <modgui/stylesheet-cv-to-audio.css> ;
        modgui:screenshot <modgui/mod_cv_to_audio_ss.png> ;
        modgui:thumbnail <modgui/mod_cv_to_audio_tn.png> ;
        modgui:brand "MOD" ;
        modgui:label "CV to audio" ;
        modgui:license "CC BY-NC-ND 4.0";
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Level" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Offset" ;
            lv2:name "Offset" ;
        ] ;
    ] .
