@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix modgui: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<http://moddevices.com/plugins/mod-devel/mod-phase-oscillator> 
a lv2:Plugin, mod:ControlVoltagePlugin;

doap:name "Phase Oscillator";

doap:developer [
    foaf:name "Bram Giesen";
    foaf:homepage <>;
    foaf:mbox <mailto:bram@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "MOD";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:jarno@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """
An oscillator using phase distortion.
""";

lv2:port
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 0;
    lv2:symbol "CVIn";
    lv2:name "CV Input";
],
[
    a lv2:InputPort, lv2:CVPort;
    lv2:index 1;
    lv2:symbol "CVRetrigger";
    lv2:name "CV Retrigger";
],
[
    a lv2:OutputPort, lv2:AudioPort;
    lv2:index 2;
    lv2:symbol "AudioOut";
    lv2:name "Audio Output";
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "FineTune" ;
    lv2:name "Fine Tune";
    lv2:default 0.0 ;
    lv2:minimum -10.0 ;
    lv2:maximum 10.0 ;
],
[
    a lv2:InputPort ,
    lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "Control" ;
    lv2:name "Control";
    lv2:default 0.1 ;
    lv2:minimum 0.1 ;
    lv2:maximum 0.93 ;
],
[
   a lv2:InputPort ,
   lv2:ControlPort ;
   lv2:index 5 ;
   lv2:symbol "Slope_to_sine" ;
   lv2:name "Slope To Sine" ;
   lv2:default 0.0 ;
   lv2:minimum 0.0 ;
   lv2:maximum 1.0 ;
   lv2:portProperty lv2:toggled;
]
.
