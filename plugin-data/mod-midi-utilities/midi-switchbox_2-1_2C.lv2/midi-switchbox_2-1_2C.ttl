@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix mod:  <http://moddevices.com/ns/mod#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://moddevices.com/plugins/mod-devel/midi-switchbox_2-1_2C>
        a mod:MIDIPlugin ,
            lv2:UtilityPlugin ,
            lv2:Plugin ;
        doap:name "MIDI SwitchBox 2-1 2C" ;
        doap:license "GPLv2+" ;
        rdfs:comment """
The SwitchBox 2-1 2C allows you to route one of the dual MIDI outputs to the dual MIDI output of the plugin.

Switch the routing on the fly by mapping the source control.

Features:
Modeled by MOD Devices
""" ;

        lv2:minorVersion 2 ;
        lv2:microVersion 0 ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 0 ;
                lv2:symbol "source" ;
                lv2:name "Source" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ,
                                 lv2:enumeration ;
                lv2:scalePoint [
                        rdfs:label "Port 1-2" ;
                        rdf:value 0 ;
                ] , [
                        rdfs:label "Port 3-4" ;
                        rdf:value 1 ;
                ] ;
        ] , [
                a lv2:InputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 1 ;
                lv2:symbol "in1" ;
                lv2:name "In 1" ;
        ] , [
                a lv2:InputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 2 ;
                lv2:symbol "in2" ;
                lv2:name "In 2" ;
        ] , [
                a lv2:InputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 3 ;
                lv2:symbol "in3" ;
                lv2:name "In 3" ;
        ] , [
                a lv2:InputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 4 ;
                lv2:symbol "in4" ;
                lv2:name "In 4" ;
        ] , [
                a lv2:OutputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 5 ;
                lv2:symbol "out1" ;
                lv2:name "Out 1" ;
        ] , [
                a lv2:OutputPort ,
                        atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:index 6 ;
                lv2:symbol "out2" ;
                lv2:name "Out 2" ;
        ] ;

        doap:developer [
            foaf:name "Jarno Verheesen & Bram Giesen" ;
            foaf:homepage <http://moddevices.com> ;
            foaf:mbox <j.verheesen@hotmail.nl> ;
        ] ;

        doap:maintainer [
            foaf:name "MOD" ;
            foaf:homepage <http://moddevices.com> ;
            foaf:mbox <mailto:devel@moddevices.com> ;
        ] ;

        mod:brand "MOD" ;
        mod:label "MIDI SwitchBox 2-1 2C" .
